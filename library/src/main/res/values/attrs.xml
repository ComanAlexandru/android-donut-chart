<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DonutProgressView">
        <attr name="donut_strokeWidth" format="dimension"/>
        <attr name="donut_bgLineColor" format="color"/>
        <attr name="donut_gapAngle" format="float"/>
        <attr name="donut_direction" format="enum">
            <enum name="clockwise" value="0"/>
            <enum name="anticlockwise" value="1"/>
        </attr>
        <attr name="donut_animateChanges" format="boolean"/>
        <attr name="donut_animationInterpolator" format="reference"/>
        <attr name="donut_animationDuration" format="integer"/>
    </declare-styleable>
</resources>
